{
  "name": "mhtdoc-to-html",
  "version": "0.1.2",
  "description": "Convert Altchunks Word documents (.docx file) to HTML format.",
  "main": "dist/mhtdoc-to-html.js",
  "types": "dist/mhtdoc-to-html.d.ts",
  "dependencies": {
    "adm-zip": "^0.5.3",
    "jsdom": "^16.7.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/jest": "^26.0.24",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" --write \"test/**/*.ts\"",
    "build": "rimraf dist/ && tsc",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drawmoon/mhtdoc-to-html.git"
  },
  "keywords": [
    "mht",
    "docx",
    "html",
    "mht-to-html",
    "mht-parse",
    "mhtdoc-to-html"
  ],
  "author": "1340260725@qq.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drawmoon/mhtdoc-to-html/issues"
  },
  "homepage": "https://github.com/drawmoon/mhtdoc-to-html#readme"
}
